<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="oval">
            <corners android:radius="5dp" ></corners>
            <stroke android:color="#575757" android:dashWidth="5dp"></stroke>
            <gradient android:startColor="@color/greenGradient2" android:endColor="@color/greenGradient2" android:type="linear" android:angle="360"></gradient>

        </shape>
    </item>
    <item>
    <shape android:shape="oval">
        <corners android:radius="5dp" ></corners>
        <stroke android:color="#575757" android:dashWidth="5dp"></stroke>
        <gradient android:startColor="@color/greenGradient2" android:endColor="@color/greenGradient3" android:type="linear" android:angle="360"></gradient>

    </shape>
</item>
</selector>